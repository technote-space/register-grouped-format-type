  "author": {
    "name": "Technote",
    "email": "technote.space@gmail.com",
    "url": "https://technote.space"
  },
  "license": "GPL-3.0",
  "keywords": [
    "gutenberg",
    "rich text",
    "dropdown"
  ],
  "homepage": "https://github.com/technote-space/register-grouped-format-type#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/technote-space/register-grouped-format-type.git"
  },
  "bugs": {
    "url": "https://github.com/technote-space/register-grouped-format-type/issues"
  },
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "dependencies": {
    "@technote-space/gutenberg-utils": "^2.2.6",
    "classnames": "^2.2.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@technote-space/gutenberg-test-helper": "^0.0.17",
    "@wordpress/block-editor": "^4.3.1",
    "@wordpress/components": "^10.0.1",
    "@wordpress/data": "^4.22.1",
    "@wordpress/element": "^2.16.0",
    "@wordpress/hooks": "^2.9.0",
    "@wordpress/rich-text": "^3.20.1",
    "babel-jest": "^26.1.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.4.0",
    "eslint-plugin-react": "^7.20.3",
    "jest": "^26.1.0",
    "react": "^16.13.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "yarn build",
    "test": "yarn lint && yarn cover",
    "test:update": "yarn lint && yarn cover:update",
    "lint": "eslint 'src/**/*.js' '__tests__/**/*.js' --cache",
    "lint:fix": "eslint --fix 'src/**/*.js' '__tests__/**/*.js'",
    "cover": "jest --coverage",
    "cover:update": "jest --coverage --updateSnapshot",
    "develop": "rm -rdf build && babel src --out-dir build",
    "build": "rm -rdf build && babel --compact=true src --out-dir build",
    "update": "npx npm-check-updates -u && yarn install && yarn upgrade && yarn audit"
  }
}
